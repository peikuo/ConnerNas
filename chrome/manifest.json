{
  "manifest_version": 3,
  "name": "CornerNAS",
  "version": "0.1.0",
  "description": "Discover CornerNAS devices on your LAN and upload/download files.",
  "action": {
    "default_title": "CornerNAS",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "downloads"],
  "host_permissions": ["http://*/*"],
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  }
}
